(define (japanese-init)
  (for-each
   (lambda (x)
     (make-item-alias (car x) (cadr x)))
   '(("wakizashi" "short sword")
     ("ninja-to" "broadsword")
     ("nunchaku" "flail")
     ("naginata" "glaive")
     ("osaku" "lock pick")
     ("koto" "wooden harp")
     ("shito" "knife")
     ("tanko" "plate mail")
     ("kabuto" "helmet")
     ("yugake" "leather gloves")
     ("gunyoki" "food ration")
     ("potion of sake" "potion of booze")
     ("potions of sake" "potions of booze"))))